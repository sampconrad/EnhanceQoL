package-as: EnhanceQoL

externals:
  EnhanceQoL/libs/LibDBIcon-1.0:
    url: https://repos.curseforge.com/wow/libdbicon-1-0/trunk
    tag: latest
  EnhanceQoL/libs/LibDataBroker-1.1:
    url: https://github.com/tekkub/libdatabroker-1-1
    tag: latest

enable-nolib-creation: no

ignore:
  - .DS_Store
  - .env
  - .git
  - .github
  - .gitignore
  - .luacheckrc
  - .release
  - scripts
  - .vscode
  - agents
  - docs
  - News
  - DESCRIPTION.md
  - Features.md
  - generate_changelog.sh
  - LICENSE
  - README.md
  - CONTRIBUTING.md
  - stylua.toml
  - update_translations.sh
  - wow-ui-retail.skill

manual-changelog:
  filename: CHANGELOG.md
  markup-type: markdown

move-folders:
  EnhanceQoL/EnhanceQoL: EnhanceQoL
  EnhanceQoL/EnhanceQoLAura: EnhanceQoLAura
  EnhanceQoL/EnhanceQoLCombatMeter: EnhanceQoLCombatMeter
  EnhanceQoL/EnhanceQoLDrinkMacro: EnhanceQoLDrinkMacro
  EnhanceQoL/EnhanceQoLMover: EnhanceQoLMover
  EnhanceQoL/EnhanceQoLMouse: EnhanceQoLMouse
  EnhanceQoL/EnhanceQoLMythicPlus: EnhanceQoLMythicPlus
  EnhanceQoL/EnhanceQoLSharedMedia: EnhanceQoLSharedMedia
  EnhanceQoL/EnhanceQoLSound: EnhanceQoLSound
  EnhanceQoL/EnhanceQoLTooltip: EnhanceQoLTooltip
  EnhanceQoL/EnhanceQoLVendor: EnhanceQoLVendor
